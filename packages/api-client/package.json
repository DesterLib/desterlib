{
  "name": "@dester/api-client",
  "version": "1.0.0",
  "description": "Production-ready TypeScript API client with Zod validation, modular endpoints, and full type safety",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dester",
    "api",
    "client",
    "media",
    "library",
    "typescript",
    "zod",
    "type-safe"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false
}
